#include <stdio.h>

int main() {
    int n, i, num;

    // Get the number of elements in the set
    printf("Enter the number of elements: ");
    scanf("%d", &n);

    // Loop through each number
    for (i = 1; i <= n; i++) {
        printf("Enter number %d: ", i);
        scanf("%d", &num);

        if (num % 2 == 0) {
            printf("%d is Even\n", num);
        } else {
            printf("%d is Odd\n", num);
        }
    }

    return 0;
}
