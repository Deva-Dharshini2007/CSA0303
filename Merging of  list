#include <stdio.h>

void mergeLists(int list1[], int size1, int list2[], int size2, int merged[]) {
    for (int i = 0; i < size1; i++) {
        merged[i] = list1[i];
    }
    for (int i = 0; i < size2; i++) {
        merged[size1 + i] = list2[i];
    }
}

int main() {
    int list1[] = {1, 3, 5};
    int list2[] = {2, 4, 6};
    int size1 = sizeof(list1) / sizeof(list1[0]);
    int size2 = sizeof(list2) / sizeof(list2[0]);

    int merged[size1 + size2];

    mergeLists(list1, size1, list2, size2, merged);

    printf("Merged List: ");
    for (int i = 0; i < size1 + size2; i++) {
        printf("%d ", merged[i]);
    }
    printf("\n");

    return 0;
}
